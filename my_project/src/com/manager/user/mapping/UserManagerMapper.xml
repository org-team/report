<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.manager.user.dao.UserManagerMapper">
<!--如果要用对象传取值 "com.demo.po.User"-->
	<select id="selectUserId_paging" resultType="Map" parameterType="com.manager.user.po.User">
	  SELECT id from t_crd_user
	</select>

	<insert id="insertUserId" parameterType="String">  
